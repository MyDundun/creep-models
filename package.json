{
  "version": "0.0.1",
  "name": "creep-models",
  "author": "CA Bezuidenhout",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1"
  },
  "scripts": {
    "build": "npm run compile-displayCreepData | npm run compile-main | npm run compile-displayIsoStress",
    "watch-displayCreepData": "npx babel DisplayCreepData/displayCreepDataTemplate/js/script.js --watch --out-file DisplayCreepData/displayCreepDataTemplate/compiled.js",
    "watch-main" : "npx babel assets/js/main.js --watch --out-file assets/js/main-compiled.js",  
    "watch-displayIsoStress" : "npx babel DisplayIsoStress/displayIsoStressTemplate/js/script.js --watch --out-file DisplayIsoStress/displayIsoStressTemplate/compiled.js",
    "compile-displayCreepData": "npx babel DisplayCreepData/displayCreepDataTemplate/js/script.js --out-file DisplayCreepData/displayCreepDataTemplate/compiled.js",
    "compile-main" : "npx babel assets/js/main.js --out-file assets/js/main-compiled.js",
    "compile-displayIsoStress" : "npx babel DisplayIsoStress/displayIsoStressTemplate/js/script.js --out-file DisplayIsoStress/displayIsoStressTemplate/compiled.js"
    
  } 
}